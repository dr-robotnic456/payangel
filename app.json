{
	"expo": {
		"name": "payAngel",
		"slug": "payAngel",
		"version": "1.0.17",
		"orientation": "portrait",
		"icon": "./src/assets/icon.png",
		"userInterfaceStyle": "light",
		"splash": {
			"image": "./src/assets/splash.png",
			"resizeMode": "contain",
			"backgroundColor": "#ffffff"
		},
		"plugins": [
			[
				"expo-barcode-scanner",
				{
					"cameraPermission": "Allow PayAngel to access camera."
				}
			]
		],
		"assetBundlePatterns": ["**/*"],
		"ios": {
			"supportsTablet": true,
			"infoPlist": {
				"NSCameraUsageDescription": "Allow PayAngel to access camera.",
				"NSMicrophoneUsageDescription": "We needs Microphone library permissions to allow you to record sound."
			},
			"bundleIdentifier": "com.payAngel",
			"buildNumber":"3"
		},
		"android": {
			"adaptiveIcon": {
				"foregroundImage": "./src/assets/adaptive-icon.png",
				"backgroundColor": "#ffffff"
			},
			"permissions": ["android.permission.CAMERA"],
			"package": "com.payAngel"
		},
		"web": {
			"favicon": "./src/assets/favicon.png"
		},
		"extra": {
			"eas": {
				"projectId": "5bf632e6-fa36-41c4-9db3-1a23bf76b62e"
			}
		},
		"owner": "teamalfy-mobile",
		"runtimeVersion": {
			"policy": "sdkVersion"
		},
		"updates": {
			"url": "https://u.expo.dev/5bf632e6-fa36-41c4-9db3-1a23bf76b62e"
		}
	}
}
